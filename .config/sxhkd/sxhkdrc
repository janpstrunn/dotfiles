# Tmux Session Manager
super + s; {shift + o,o,c,g}
  {__alt-term.sh,__tmuxp-open.sh,__tmuxp-close.sh,buku_run}

# Screenshooter
super + g;
  flameshot gui

# Browser
super + F1;
  zen-browser -P

# Explorer
super + e;
  kitty -e ranger

# Rofi run
super + r;
  rofi -modes drun,run -show run

# Obsidian Quick Switcher

super + o
  __rofi-obsidian.sh o

super + shift + o
  __rofi-obsidian.sh n

super + control + o
  __switch-workspace.sh

# Rofi
super + d; {p,t,d}
  {__passmenu-rofi.sh,__passmenu-otp.sh,rofi -lines 5 -eh 3 -show ddgrsearch -modi "ddgrsearch:ddgr_search.py"}

# Call
super + c; {c,n,shift + n,o,h,p,r,f,d}
  {__clearboard.sh,cmus-remote -n,cmus-remote -r,__obsidian-daily.sh -s,obsidian-cli open Homepage --vault HEPHAESTUS,__cmus-player-check.sh,obs,./beelzebub/Pandora/applications/app-freetube.AppImage}

super + y
  __download-youtube.sh -c
