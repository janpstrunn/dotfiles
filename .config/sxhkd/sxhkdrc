# Tmux Session Manager
super + s; {o,c,g,p}
  {__tmuxp-open.sh,__tmuxp-close.sh,buku_run,__pomodoro.sh}

# Screenshooter
super + g;
  flameshot gui

# Browser
super + F1;
  zen-browser
  zen-browser -P

# Explorer
super + e;
  ghostty -e ranger

# Rofi run
super + r;
  rofi -modes drun,run -show run

# Obsidian Quick Switcher

super + o
  __rofi-obsidian.sh o

super + shift + o
  __rofi-obsidian.sh n

super + control + o
  __switch-workspace.sh

# Rofi
super + d; {p,t}
  {__passmenu-rofi.sh,__passmenu-otp.sh}

# Call
super + c; {c,n,shift + n,o,h,p,r,f}
  {__clearboard.sh,cmus-remote -n,cmus-remote -r,__obsidian-daily.sh -s,obsidian-cli open Homepage --vault HEPHAESTUS,__cmus-player-check.sh,obs,__app-opener.sh f}

super + y
  __download-youtube.sh -c
