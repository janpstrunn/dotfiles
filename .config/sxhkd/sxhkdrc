# Tmux Session Manager
super + c;
  {rofi-tmux}

super + alt + c;
  __clearboard

super + d;
  rofipass -f

super + D;
  pass.sh -r

super + y;
  yt-download -a

super + b;
  bash -c "~/.local/bin/bmark -r list | rofi -dmenu | xargs -I {} xdg-open '{}'

# Screenshooter
super + g;
  flameshot gui

# Browser
super + F1;
  zen-browser

# Explorer
super + e;
  st -e ranger

super + 0;
  freetube

# Rofi run
super + r;
  rofi -modes drun,run -show run

# Obsidian Quick Switcher
super + o
  __rofi-obsidian.sh o

# Obsidian Quick Switcher for neovim
super + shift + o
  __rofi-obsidian.sh n

# DuckDuckGo
super + shift + y
  rofi -lines 5 -eh 2 -show ddgrsearch -modi "ddgrsearch:ddgr_search.py"

# Switch Obsidian Workspace
super + control + o
  __switch-workspace.sh

# Rofi
super + d; {p,t}
  {__passmenu-rofi.sh,__passmenu-otp.sh}

# Call
super + c; {c,n,shift + n,o,h,p,r,f}
  {__clearboard.sh,cmus-remote -n,cmus-remote -r,__obsidian-daily.sh -s,obsidian-cli open Homepage --vault HEPHAESTUS,__cmus-player-check.sh,obs,__app-opener.sh}

# Calc
super + shift + c
  rofi -show calc -modi calc -no-show-match -no-sort > /dev/null

# Quick Audio Download
super + y
  __download-youtube.sh -a
