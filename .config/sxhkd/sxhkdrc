# Tmux Session Manager

super + s; {o,c}
  {__tmuxp-open.sh,__tmuxp-close.sh}

# Screenshooter
super + g;
  flameshot gui

# Browser
super + F1;
  flatpak run io.github.zen_browser.zen -P

# Explorer
super + e;
  kitty -e ranger

# Rofi run
super + r;
  rofi -show run

# Rofi
super + d; {o,p,t,d}
  {__rofi-obsidian.sh,__passmenu-rofi.sh,__passmenu-otp.sh,rofi -lines 5 -eh 3 -show ddgrsearch -modi "ddgrsearch:ddgr_search.py"}

# Call
super + c; {c,n,shift + n,o,p,r}
  {__clearboard.sh,cmus-remote -n,cmus-remote -r,obsidian,__cmus-player-check.sh,obs}
